ios'\r\nexport default {\r\n  name: 'CreateReview',\r\n  data(){\r\n    return {\r\n      title: '',\r\n      content: '',\r\n    }\r\n  },\r\n  methods:{\r\n    setToken: function () {\r\n      const token = localStorage.getItem('jwt')\r\n      const config = {\r\n        Authorization: `JWT ${token}`\r\n      }\r\n      return config\r\n    },\r\n    onClick(){\r\n      const movie_id=this.movie.movie_id\r\n      const review_id=this.selectReview.id\r\n      const Review = {\r\n        title:this.title,\r\n        content:this.content\r\n      }\r\n      axios({\r\n        method:'PUT',\r\n        url: `http://127.0.0.1:8000/review/${movie_id}/${review_id}/update/`,\r\n        headers: this.setToken(),\r\n        data: Review,\r\n      })\r\n        .then(res =>{\r\n          console.log(res.data)\r\n          const review = res.data\r\n          review['movie']-=1\r\n          this.$store.state.selectReview = review\r\n          this.$router.push({name : 'ReviewDetail'})\r\n        })\r\n        .catch(err => {\r\n          console.log(err)\r\n        })\r\n    }\r\n  },\r\n  created(){\r\n    this.title=this.selectReview.title\r\n    this.content=this.selectReview.content\r\n  },\r\n  computed:{\r\n    movie(){\r\n      return this.$store.state.movies[this.selectReview.movie]\r\n    },\r\n    posterPath(){\r\n      return 'https://image.tmdb.org/t/p/w500/' + this.movie.poster_path\r\n    },\r\n    selectReview(){\r\n      return this.$store.state.selectReview\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>"]}]}