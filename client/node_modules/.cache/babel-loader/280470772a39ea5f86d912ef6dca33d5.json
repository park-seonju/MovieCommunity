A,OAPA;AAQA,KAjBA;AAkBA,IAAA,UAlBA,sBAkBA,cAlBA,EAkBA;AAAA;;AACA,UAAA,MAAA,GAAA;AACA,QAAA,MAAA,EAAA,KADA;AAEA,QAAA,GAAA,iCAFA;AAGA,QAAA,OAAA,EAAA;AACA,UAAA,aAAA,gBAAA,YAAA,CAAA,OAAA,CAAA,KAAA,CAAA;AADA;AAHA,OAAA;AAOA,MAAA,KAAA,CAAA,MAAA,CAAA,CACA,IADA,CACA,UAAA,GAAA,EAAA;AACA,QAAA,GAAA,CAAA,IAAA,CAAA,OAAA,CAAA,UAAA,MAAA,EAAA;AACA,cAAA,MAAA,CAAA,EAAA,KAAA,cAAA,EAAA;AACA,YAAA,MAAA,CAAA,KAAA,IAAA,CAAA;AACA,YAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,YAAA,GAAA,MAAA;;AACA,YAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,cAAA,IAAA,EAAA;AAAA,aAAA;AACA;AACA,SANA;AAOA,OATA,EASA,KATA,CASA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAXA;AAYA;AAtCA,GAVA;AAkDA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,aAAA,KAAA,MAAA,CAAA,OAAA,CAAA,eAAA;AACA;AAHA,GAlDA;AAuDA,EAAA,OAvDA,qBAuDA;AACA,SAAA,UAAA;AACA;AAzDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <h1 class=\"header\">My Page</h1>\r\n    <div class=\"mytotal\">\r\n      <div class=\"total mypagetotal row\">\r\n        <div class=\"bbig col-12 col-lg-6\">\r\n          <span class=\"reviewcomment\">작성 리뷰</span>\r\n          <ul v-for=\"review in myReviews\" :key=\"review.id\" class=\"write_review_comment\">\r\n            <a href=\"#\" @click.prevent=\"moveReview(review.id)\">\r\n              {{review.title}}\r\n            </a>\r\n            <!-- <button class=\"btn btn-primary btn-sm originSee\" @click=\"\">원문 보기</button> -->\r\n          </ul>\r\n        </div>\r\n        <div class=\"bbig col-12 col-lg-6\">\r\n          <span class=\"reviewcomment\">작성 댓글</span>\r\n          <ul v-for=\"comment in myComments\" :key=\"comment.id\" class=\"write_review_comment\">\r\n            <a href=\"#\" @click.prevent=\"moveReview(comment.review)\">\r\n              {{ comment.content }}\r\n            </a>\r\n            <!-- <button class=\"btn btn-primary btn-sm originSee\" @click=\"\">원문 보기</button> -->\r\n          </ul>\r\n        </div>\r\n      </div>\r\n      <div class=\"like_movies\">\r\n        <h2>좋아요 누른 영화</h2>\r\n        <MultipleCarousel :movies=\"myMovies\" :modalName=\"'myPage'\"/>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport MultipleCarousel from '@/components/MultipleCarousel.vue'\r\nimport axios from 'axios'\r\nexport default {\r\n  components:{\r\n    MultipleCarousel\r\n  },\r\n  data(){\r\n    return {\r\n      myReviews:[],\r\n      myComments:[],\r\n    }\r\n  },\r\n  methods:{\r\n    getProfile(){\r\n      const params = {\r\n        method: 'get',\r\n        url:`http://127.0.0.1:8000/accounts/profile/`,\r\n        headers: {\r\n          Authorization: `JWT ${localStorage.getItem('jwt')}`, \r\n        },\r\n      }\r\n      axios(params)\r\n        .then((res)=>{\r\n        this.myReviews = res.data.reviews\r\n        this.myComments = res.data['comments']\r\n        this.$store.dispatch('getMyPageMovies', res.data['like_movies'])\r\n      }).catch((e)=>{\r\n        console.log(e)\r\n      })\r\n    },\r\n    moveReview(comment_review){\r\n      const params = {\r\n        method: 'get',\r\n        url:`http://127.0.0.1:8000/review/`,\r\n        headers: {\r\n          Authorization: `JWT ${localStorage.getItem('jwt')}`, \r\n        },\r\n      }\r\n      axios(params)\r\n        .then((res)=>{\r\n        res.data.forEach(review => {\r\n          if(review.id===comment_review){\r\n            review.movie-=1\r\n            this.$store.state.selectReview = review\r\n            this.$router.push({name: 'ReviewDetail'})\r\n          }\r\n        });\r\n      }).catch((e)=>{\r\n        console.log(e)\r\n      })\r\n    },\r\n  },\r\n  computed:{\r\n    myMovies(){\r\n      return this.$store.getters.getMyPageMovies\r\n    },\r\n  },\r\n  created(){\r\n    this.getProfile()\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\nh2{\r\n  color:#fff;\r\n}\r\n.like_movies{\r\n  margin-top:30px;\r\n}\r\n.header{\r\n  color: rgb(202, 201, 202);\r\n  font-size:50px;\r\n  margin: 30px;\r\n}\r\n.reviewcomment{\r\n  font-size: 25px;\r\n  margin-top: 1vh;\r\n}\r\n.write_review_comment{\r\n  display: flex;\r\n  margin-top:25px;\r\n}\r\n.mypagetotal{\r\n  display: flex;\r\n  flex-direction: row;\r\n}\r\n.bbig{\r\n  display: flex;\r\n  flex-direction: column;\r\n  /* width:50%; */\r\n}\r\n.bbig a{\r\n  color: #fff;\r\n}\r\n.bbig a:hover{\r\n  color: #ddd;\r\n}\r\n\r\n.originSee{\r\n  margin-left: 15px;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}