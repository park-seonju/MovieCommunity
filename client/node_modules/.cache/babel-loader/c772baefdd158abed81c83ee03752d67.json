AAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AACA,QAAA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,YAAA,GAAA,MAAA;;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,UAAA,IAAA,EAAA;AAAA,SAAA;AACA,OANA,EAOA,KAPA,CAOA,UAAA,CAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,CAAA;AACA,OATA;AAUA;AA5BA,GATA;AAuCA,EAAA,OAAA,EAAA,EAvCA;AAwCA,EAAA,QAAA,EAAA;AACA,IAAA,KADA,mBACA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAIA;AACA,aAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,QAAA;AACA,KANA;AAOA,IAAA,UAPA,wBAOA;AACA,aAAA,qCAAA,KAAA,MAAA,CAAA,KAAA,CAAA,WAAA,CAAA,WAAA;AACA;AATA;AAxCA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <form>\r\n      <div class=\"total\">\r\n        <p class=\"mv_title\">영화: {{ movie.title }}</p>\r\n        <p><label for=\"title\">제목</label></p>\r\n        <p><input type=\"text\" id=\"title\" style=\"width:40%; padding:10px;\" v-model=\"title\"></p>\r\n        <p><label for=\"content\">리뷰 작성</label></p>\r\n        <p><textarea type=\"text\" id=\"content\" v-model=\"content\" style=\"width:80%; padding:10px; resize:none;\" rows=15px></textarea></p>\r\n        <fieldset class=\"star_button\">\r\n          <p class=\"scoretext\">평점 : </p>\r\n          <span class=\"star-cb-group\" >\r\n            <input type=\"radio\" id=\"rating-5\" name=\"rating\" value=\"5\" v-model=\"score\" checked=\"checked\"/>\r\n            <label for=\"rating-5\">5</label>\r\n            <input type=\"radio\" id=\"rating-4\" name=\"rating\" value=\"4\" v-model=\"score\"/>\r\n            <label for=\"rating-4\">4</label>\r\n            <input type=\"radio\" id=\"rating-3\" name=\"rating\" value=\"3\" v-model=\"score\"/>\r\n            <label for=\"rating-3\">3</label>\r\n            <input type=\"radio\" id=\"rating-2\" name=\"rating\" value=\"2\" v-model=\"score\"/>\r\n            <label for=\"rating-2\">2</label>\r\n            <input type=\"radio\" id=\"rating-1\" name=\"rating\" value=\"1\" v-model=\"score\"/>\r\n            <label for=\"rating-1\">1</label>\r\n            <input type=\"radio\" id=\"rating-0\" name=\"rating\" value=\"0\" v-model=\"score\" class=\"star-cb-clear\" />\r\n            <label for=\"rating-0\">0</label>\r\n          </span>\r\n          <button class=\"btn btn-primary createbtn\" @click.prevent=\"onClick\">작성</button>   \r\n        </fieldset>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nexport default {\r\n  name: 'CreateReview',\r\n  data(){\r\n    return {\r\n      title: '',\r\n      content: '',\r\n      score: '',\r\n    }\r\n  },\r\n  methods:{\r\n    onClick(){\r\n      const token = localStorage.getItem('jwt')\r\n      const data = {\r\n        title: this.title,\r\n        content: this.content,\r\n        score: this.score,\r\n      }\r\n      const movie_id = this.$store.state.selectMovie.movie_id\r\n      const URL =  `http://127.0.0.1:8000/review/${movie_id}/`\r\n      const params = {\r\n        method: 'post',\r\n        url: URL,\r\n        data: data,\r\n        headers: {\r\n          Authorization: `JWT ${token}`, \r\n        },\r\n      }\r\n      axios(params)\r\n        .then((res)=>{\r\n          const review = res.data\r\n          review['movie'] -= 1\r\n          this.$store.state.selectReview = review\r\n          this.$router.push({name:'ReviewDetail'})\r\n        })\r\n        .catch((e)=>{\r\n          console.log(e)\r\n        })\r\n    },\r\n  },\r\n  created:{},\r\n  computed:{\r\n    movie(){\r\n      return this.$store.state.selectMovie\r\n    },\r\n    movieId(){\r\n      return this.$store.state.selectMovie.movie_id\r\n    },\r\n    posterPath(){\r\n      return 'https://image.tmdb.org/t/p/w500/' + this.$store.state.selectMovie.poster_path\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.mv_title{\r\n  font-size:30px;\r\n}\r\ntextarea{\r\n  border-radius: 20px;\r\n}\r\n.star_button{\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  margin-right:95px;\r\n}\r\n.createbtn{\r\n  margin-left:20px;\r\n}\r\n.scoretext{\r\n  margin-top:25px;\r\n  margin-right:10px;\r\n}\r\n\r\n.star-cb-group {\r\n  font-size: 0;\r\n  unicode-bidi: bidi-override;\r\n  direction: rtl;\r\n}\r\n.star-cb-group * {\r\n  font-size: 1rem;\r\n}\r\n.star-cb-group > input {\r\n  display: none;\r\n}\r\n.star-cb-group > input + label {\r\n  display: inline-block;\r\n  overflow: hidden;\r\n  width: 1em;\r\n  white-space: nowrap;\r\n  cursor: pointer;\r\n  font-size:40px;\r\n}\r\n.star-cb-group > input + label:before {\r\n  display: inline-block;\r\n  content: \"☆\";\r\n  color: #888;\r\n}\r\n.star-cb-group > input:checked ~ label:before, .star-cb-group > input + label:hover ~ label:before, .star-cb-group > input + label:hover:before {\r\n  content: \"★\";\r\n  color: gold;\r\n  text-shadow: 0 0 1px #333;\r\n}\r\n.star-cb-group > .star-cb-clear + label {\r\n  text-indent: -9999px;\r\n  width: .5em;\r\n  margin-left: -.5em;\r\n}\r\n.star-cb-group > .star-cb-clear + label:before {\r\n  width: .5em;\r\n}\r\n.star-cb-group:hover > input + label:before {\r\n  content: \"☆\";\r\n  color: #888;\r\n  text-shadow: none;\r\n}\r\n.star-cb-group:hover > input + label:hover ~ label:before, .star-cb-group:hover > input + label:hover:before {\r\n  content: \"★\";\r\n  color: gold;\r\n  text-shadow: 0 0 1px #333;\r\n}\r\n\r\n</style>"],"sourceRoot":"src/components"}]}